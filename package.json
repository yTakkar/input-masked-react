{
  "name": "input-masked-react",
  "version": "0.3.27",
  "description": "A fully customizable masked input component for the web built with React",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "build:watch": "babel src -w -d lib",
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "demo": "parcel demo/index.html -p 4000 -d demo/dist"
  },
  "keywords": [
    "masking",
    "Masked",
    "input",
    "DOB",
    "OTP",
    "CCV"
  ],
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.13.4",
    "prop-types": "^15.7.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "author": "Faiyaz Shaikh <www.shtakkar@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yTakkar/input-masked-react"
  },
  "bugs": {
    "url": "https://github.com/yTakkar/input-masked-react/issues"
  }
}
